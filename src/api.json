{
  "name": "Twitter",
  "apiVersion": "1.1",
  "baseUrl": "https://demo-api.ig.com/gateway/deal/",
  "operations": {
    "Login": {
      "httpMethod": "POST",
      "uri": "session",
      "responseClass": "LoginOutput",
      "parameters": {
        "identifier": {
          "location": "json"
        },
        "password": {
          "location": "json"
        },
        "apiKey": {
          "location": "header",
          "sentAs": "X-IG-API-KEY"
        },
        "version": {
          "location": "header",
          "sentAs": "Version",
          "default": "3"
        }
      }
    },
    "GetPrices": {
      "httpMethod": "GET",
      "uri": "prices/{epic}",
      "responseClass": "LoginOutput",
      "parameters": {
        "epic": {
          "location": "uri"
        },
        "resolution": {
          "location": "query",
          "default": "MINUTE_5"
        },
        "startDate": {
          "location": "query",
          "sentAs": "from"
        },
        "endDate": {
          "location": "query",
          "sentAs": "to"
        },
        "maxPricePoints": {
          "location": "query",
          "sentAs": "max",
          "default": "100"
        },
        "pageSize": {
          "location": "query",
          "default": "10"
        },
        "pageNumber": {
          "location": "query",
          "default": "1"
        }
      }
    }
  },
  "models": {
    "LoginOutput": {
      "type": "object",
      "additionalProperties": {
        "location": "json"
      }
    }
  }
}
